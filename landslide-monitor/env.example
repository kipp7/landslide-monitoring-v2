# 滑坡监测系统环境配置模板
# 复制此文件为 .env 并填写实际值

# 应用配置
NODE_ENV=development
PORT=5100
DEBUG=true

# Supabase 配置 (必需)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# 前端 Supabase 配置 (必需)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# WebSocket 配置
WS_PORT=5101
WS_HEARTBEAT_INTERVAL=30000

# 数据库配置
DB_CLEANUP_INTERVAL=3600000
DB_MAX_RECORDS=10000

# 华为云 IoT 配置 (可选，已禁用)
# HUAWEI_IOT_ENDPOINT=your_huawei_iot_endpoint
# HUAWEI_IOT_PROJECT_ID=your_project_id  
# HUAWEI_IOT_DEVICE_ID=your_device_id

# GPS 配置
GPS_ACCURACY_THRESHOLD=2.5
GPS_BASELINE_CONFIDENCE=0.95

# 机器学习配置
ML_ENABLED=true
ML_MODEL_UPDATE_INTERVAL=86400000
ML_PREDICTION_THRESHOLD=0.8
