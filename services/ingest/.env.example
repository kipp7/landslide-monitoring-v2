# ingest-service example config (DO NOT COMMIT REAL SECRETS)

SERVICE_NAME=ingest-service

MQTT_URL=mqtt://127.0.0.1:1883
# Optional: if broker requires auth, set both username and password.
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_TOPIC_TELEMETRY=telemetry/+

# Overload protection (fail-safe to DLQ)
MESSAGE_MAX_BYTES=262144
METRICS_MAX_KEYS=500

KAFKA_BROKERS=127.0.0.1:9092
KAFKA_CLIENT_ID=ingest-service
KAFKA_TOPIC_TELEMETRY_RAW=telemetry.raw.v1
KAFKA_TOPIC_TELEMETRY_DLQ=telemetry.dlq.v1
